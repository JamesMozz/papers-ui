extends print-layout

block content
  div(class='header')
    h1(class='day' style='margin: -5px')= 'Posters'
    h2(class='track' style='margin: -5px')!=  '<strong>Tuesday Evening:</strong><em> 5:40 PM - 7:30 PM</em> '
      h3(class='room' style='text-align: center; margin: -10px')= 'Room: SCE House'
  div(class='content ')
    div(class='session-header')
      h2= 'Posters from The Creation of Big Data Labs for Archaeology, Heritage, and Global Initiatives'
    each paper in papers
      if paper.sessionType === 'ZZZ' && paper.sessionTitle === 'The Creation of Big Data Labs for Archaeology, Heritage, and Global Initiatives'
        if paper.lastName8
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6 + ', ' + paper.firstName7 + ' ' + paper.lastName7 + ', ' + paper.firstName8 + ' ' + paper.lastName8
        else if paper.lastName7
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6 + ', ' + paper.firstName7 + ' ' + paper.lastName7
        else if paper.lastName6
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6
        else if paper.lastName5
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5
        else if paper.lastName4
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4
        else if paper.lastName3
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3
        else if paper.lastName2
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2
        else if paper.lastName1
          - var authorString = paper.lastName1 + ', ' + paper.firstName1
        div(class='paper')
          case paper.title
            when 'Discussion'
              p(class='description') <em>#{paper.title}</em>
            when 'Coffee Break'
              p(class='description') <em>#{paper.title}</em>
            default
              p(class='description') <strong>#{authorString}</strong>, <em>#{paper.title}</em>
    div(class='session-header')
      h2= 'General Posters'
    each paper in papers
      if paper.sessionType === 'ZZZ' && paper.sessionTitle === 'General Posters'
        if paper.lastName8
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6 + ', ' + paper.firstName7 + ' ' + paper.lastName7 + ', ' + paper.firstName8 + ' ' + paper.lastName8
        else if paper.lastName7
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6 + ', ' + paper.firstName7 + ' ' + paper.lastName7
        else if paper.lastName6
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5 + ', ' + paper.firstName6 + ' ' + paper.lastName6
        else if paper.lastName5
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4 + ', ' + paper.firstName5 + ' ' + paper.lastName5
        else if paper.lastName4
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3 + ', ' + paper.firstName4 + ' ' + paper.lastName4
        else if paper.lastName3
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2 + ', ' + paper.firstName3 + ' ' + paper.lastName3
        else if paper.lastName2
          - var authorString = paper.lastName1 + ', ' + paper.firstName1 + ', ' + paper.firstName2 + ' ' + paper.lastName2
        else if paper.lastName1
          - var authorString = paper.lastName1 + ', ' + paper.firstName1
        div(class='paper')
          case paper.title
            when 'Discussion'
              p(class='description') <em>#{paper.title}</em>
            when 'Coffee Break'
              p(class='description') <em>#{paper.title}</em>
            default
              p(class='description') <strong>#{authorString}</strong>, <em>#{paper.title}</em>
